    <script>
        function a(num) {
            for(var i = 0; i < 5; ++i){
                if(num == i)continue;//不改变点击节点的样式，用于后面的判断
                $(".list").eq(i).css("display", "none");
            }
            if (num == 0) {
                if($(".list_father .list_0").css("display")=="block"){
                    $(".list_father .list_0").css("display","none");
                }else{
                    $(".list_father .list_0").css("display","block");
//                    $(".list_father .list_0").fadeIn("slow");//3000
                }
            }
            else if (num == 1) {
                if($(".list_father .list_1").css("display")=="block"){
                    $(".list_father .list_1").css("display","none");
                }else{
                    $(".list_father .list_1").css("display","block");
                }
            }
            else if (num == 2) {
                if($(".list_father .list_2").css("display")=="block"){
                    $(".list_father .list_2").css("display","none");
                }else{
                    $(".list_father .list_2").css("display","block");
                }
            }
            else if (num == 3) {
                if($(".list_father .list_3").css("display")=="block"){
                    $(".list_father .list_3").css("display","none");
                }else{
                    $(".list_father .list_3").css("display","block");
                }
            }
//            else if (num == 4) {
//                $(".muen ul li .list_4").css("display","block");
//            }
        }
        $(".list_father:eq(0)").mouseover(function(){a(0);});
        $(".list_father:eq(0)").mouseout(function(){a(0);});
        $(".list_father:eq(1)").mouseover(function(){a(1);});
        $(".list_father:eq(1)").mouseout(function(){a(1);});
        $(".list_father:eq(2)").mouseover(function(){a(2);});
        $(".list_father:eq(2)").mouseout(function(){a(2);});
        $(".list_father:eq(3)").mouseover(function(){a(3);});
        $(".list_father:eq(3)").mouseout(function(){a(3);});
        $(".list_father:eq(4)").mouseover(function(){a(4);});
        $(".list_father:eq(4)").mouseout(function(){a(4);});
    </script>

